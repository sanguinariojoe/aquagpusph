<?xml version="1.0" ?>

<!-- This preset replaces the time integration scheme by a 1st order Euler one.

    To use this module, just simply include it any time after basic.xml
-->

<sphInput>
    <Definitions>
        <Define name="TSCHEME" value="TSCHEME_EULER" evaluate="false"/>
    </Definitions>

    <Tools>
        <Tool action="replace" name="predictor" type="kernel" entry_point="predictor" path="../../../Scripts/basic/time_scheme/euler.cl"/>
        <Tool action="replace" name="corrector" type="kernel" entry_point="corrector" path="../../../Scripts/basic/time_scheme/euler.cl"/>
    </Tools>
</sphInput>
